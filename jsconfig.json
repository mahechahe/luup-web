{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@auth/*": [
        "src/auth/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@public/*": [
        "public/*"
      ]
    }
  }
}